{
  "name": "cumulus-dashboard",
  "version": "2.0.0",
  "description": "A dashboard for Cumulus API",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus-dashboard"
  },
  "author": {
    "name": "Cumulus OX Team"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nasa/cumulus-dashboard/issues"
  },
  "homepage": "https://github.com/nasa/cumulus-dashboard",
  "private": true,
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "audit-ci": "audit-ci --config $(pwd)/audit-ci.json",
    "serve": "NODE_ENV=development webpack-dev-server --open --config webpack.dev.js",
    "serve-hot": "NODE_ENV=development webpack-dev-server --open --hot --config webpack.dev.js",
    "serve-cypress": "SHOW_DISTRIBUTION_API_METRICS=true ESROOT=http://example.com npm run serve",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "watch": "webpack --watch",
    "lint": "eslint . --ext .js",
    "lint-fix": "eslint --fix . --ext .js",
    "cypress": "LOCALSTACK_HOST=localhost LOCAL_ES_HOST=localhost CYPRESS_TESTING=true NODE_ENV=test cypress open",
    "cypress-ci": "LOCALSTACK_HOST=localhost LOCAL_ES_HOST=localhost CYPRESS_TESTING=true NODE_ENV=test cypress run --spec 'cypress/integration/*' --config video=false",
    "validate": "ava --config ./validation-tests.config.cjs",
    "test": "NODE_ENV=test npm run lint && ava",
    "seed-database": "LOCALSTACK_HOST=localhost LOCAL_ES_HOST=localhost NODE_ENV=test node ./localAPI/runSeedDatabase.js",
    "serve-api": "LOCALSTACK_HOST=localhost LOCAL_ES_HOST=localhost NODE_ENV=test cmr_provider=CUMULUS FAKE_AUTH=true ./node_modules/@cumulus/api/bin/cli.js serve --no-reseed",
    "debug-api": "LOCALSTACK_HOST=localhost LOCAL_ES_HOST=localhost NODE_ENV=test cmr_provider=CUMULUS FAKE_AUTH=true node --inspect-brk ./node_modules/@cumulus/api/bin/cli.js serve --no-reseed",
    "start-localstack": "docker-compose -f ./localAPI/docker-compose.yml up -d",
    "stop-localstack": "docker-compose -f ./localAPI/docker-compose.yml down",
    "start-cumulusapi": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-serve-api.yml up -d",
    "stop-cumulusapi": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-serve-api.yml down",
    "start-dashboard": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-serve-api.yml -f ./localAPI/docker-compose-dashboard.yml up -d",
    "stop-dashboard": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-serve-api.yml -f ./localAPI/docker-compose-dashboard.yml down",
    "e2e-tests": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-serve-api.yml -f ./localAPI/docker-compose-dashboard.yml -f ./localAPI/docker-compose-cypress.yml up --exit-code-from=e2e --abort-on-container-exit",
    "validation-tests": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-serve-api.yml -f ./localAPI/docker-compose-dashboard.yml -f ./localAPI/docker-compose-validate.yml up --exit-code-from=validate --abort-on-container-exit",
    "view-docker-logs": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-serve-api.yml -f ./localAPI/docker-compose-dashboard.yml logs -f "
  },
  "ava": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 safari versions"
    ]
  },
